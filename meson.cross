[host_machine]
system = 'unknown'
cpu_family = 'riscv64'
cpu = 'riscv64'
endian = 'little'

[binaries]
c = 'riscv64-unknown-none-elf-gcc'
ar = 'riscv64-unknown-none-elf-ar'
strip = 'riscv64-unknown-none-elf-strip'
ld = 'riscv64-unknown-none-elf-ld'
rust = ['rustc', '--target=riscv64gc-unknown-none-elf']

[properties]
skip_sanity_check = true